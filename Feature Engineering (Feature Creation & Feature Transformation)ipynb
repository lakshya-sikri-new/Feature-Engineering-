    "# Customer Order Count\n",
    "df['customer_order_count'] = df.groupby('customer_name')['order_id'].transform('count')\n",
    "\n",
    "# Customer Lifetime Value (CLV)\n",
    "df['customer_clv'] = df.groupby('customer_name')['profit'].transform('sum')\n",
    "df.head()"
